				<section class='content-header'>
					<h1>
					<%= t(controller.controller_name).downcase.capitalize.pluralize %>
					<small><%= t(controller.action_name).downcase.capitalize %></small>
					</h1>
					<ol class='breadcrumb'>
						<li><a href='#'><i class='fa fa-globe'></i> Home</a></li>
						<li class='active'><%= t(controller.controller_name).downcase.capitalize.pluralize %></li>
					</ol>
				</section>
				<section class='content'>
					<%- unless notice.blank? %>
					<div class='alert alert-success alert-dismissable'>
						<button type='button' class='close' data-dismiss='alert' aria-hidden='true'>Ã—</button>
