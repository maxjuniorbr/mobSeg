			<aside class='main-sidebar'>
				<section class='sidebar'>
					<ul class='sidebar-menu'>
						<li>
							<%= link_to(content_tag(:i, '', class:'fa fa-users') + ' ' + t(:people).pluralize, people_path) %>
						</li>
						<li class='header'>
							<i class='fa fa-gears'></i>
							<%= t(:settings).mb_chars.upcase %>
						</li>
						<li>
							<%= link_to(content_tag(:i, '', class:'fa fa-circle-o text-aqua') + ' ' + t(:productive), productives_path) %>
